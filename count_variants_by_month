tail +2 input_by_month.tsv | cut -f 1-27 | grep -w "P.1" | sort | uniq > DC_P1.tsv
tail +2 input_by_month.tsv | cut -f 1-27 | grep -w "B.1.351" | sort | uniq > DC_B1351.tsv
tail +2 input_by_month.tsv | cut -f 1-27 | grep -w "B.1.617.2" | sort | uniq > DC_B16172.tsv
cat DC*.tsv > DC_all_variants.tsv
VOI1=$(grep -cw "B.1.427" DC_all_variants.tsv)
VOI2=$(grep -cw "B.1.429" DC_all_variants.tsv)
