VOC2=$(grep -cw "P.1" DC_all_variants.tsv)
VOC3=$(grep -cw "B.1.351" DC_all_variants.tsv)
VOC4=$(grep -cw "B.1.617.2" DC_all_variants.tsv)

echo "Variants of Interest/Concern in DC"
printf "B.1.427\t%s\t%s\n" "${VOI1}"
printf "B.1.429\t%s\t%s\n" "${VOI2}"
printf "B.1.525\t%s\t%s\n" "${VOI3}"
printf "B.1.526\t%s\t%s\n" "${VOI4}"
printf "B.1.617.1\t%s\t%s\n" "${VOI5}"
printf "B.1.617.3\t%s\t%s\n" "${VOI6}"
printf "P.2\t%s\t%s\n" "${VOI7}"

printf "B.1.1.7\t%s\t%s\n" "${VOC1}"
printf "P.1\t%s\t%s\n" "${VOC2}"
printf "B.1.351\t%s\t%s\n" "${VOC3}"
printf "B.1.617.2\t%s\t%s\n" "${VOC4}"
tail +2 DC_all_variants.tsv | cut -f 1-27 | grep -w "DC-DFS-PHL" | sort | uniq > DC_PHL.tsv
DFSVOI1=$(grep -cw "B.1.427" DC_PHL.tsv)
DFSVOI2=$(grep -cw "B.1.429" DC_PHL.tsv)
DFSVOI3=$(grep -cw "B.1.525" DC_PHL.tsv)
DFSVOI4=$(grep -cw "B.1.526" DC_PHL.tsv)
DFSVOI5=$(grep -cw "B.1.617.1" DC_PHL.tsv)
DFSVOI6=$(grep -cw "B.1.617.3" DC_PHL.tsv)
DFSVOI7=$(grep -cw "P.2" DC_PHL.tsv)
DFSVOC1=$(grep -cw "B.1.1.7" DC_PHL.tsv)
DFSVOC2=$(grep -cw "P.1" DC_PHL.tsv)
DFSVOC3=$(grep -cw "B.1.351" DC_PHL.tsv)
DFSVOC4=$(grep -cw "B.1.617.2" DC_PHL.tsv)

echo "Variants of Interest/Concern sequenced by PHL"
printf "B.1.427\t%s\t%s\n" "${DFSVOI1}"
printf "B.1.429\t%s\t%s\n" "${DFSVOI2}"
printf "B.1.525\t%s\t%s\n" "${DFSVOI3}"
printf "B.1.526\t%s\t%s\n" "${DFSVOI4}"
printf "B.1.617.1\t%s\t%s\n" "${DFSVOI5}"
printf "B.1.617.3\t%s\t%s\n" "${DFSVOI6}"
printf "P.2\t%s\t%s\n" "${DFSVOI7}"

printf "B.1.1.7\t%s\t%s\n" "${DFSVOC1}"
printf "P.1\t%s\t%s\n" "${DFSVOC2}"
printf "B.1.351\t%s\t%s\n" "${DFSVOC3}"
printf "B.1.617.2\t%s\t%s\n" "${DFSVOC4}"

